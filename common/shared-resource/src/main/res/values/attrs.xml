<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="displayTitle" format="boolean" />
    <attr name="displayHelper" format="boolean" />
    <attr name="minimalWord" format="integer" />
    <attr name="labelTitle" format="string" />
    <attr name="inputFormat" format="enum">
        <enum name="text" value="0" />
        <enum name="password" value="1" />
        <enum name="email" value="2" />
    </attr>

    <declare-styleable name="FormTextField">
        <attr name="displayTitle" />
        <attr name="displayHelper" />
        <attr name="minimalWord" />
        <attr name="labelTitle" />
        <attr name="inputFormat" />
        <attr name="android:hint" />
        <attr name="android:text" />
        <attr name="android:inputType" />
        <attr name="android:maxLines" />
        <attr name="android:lines" />
        <attr name="android:maxLength" />
    </declare-styleable>
</resources>